{
  "name": "tuffz-info-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/tuffz-info-e2e/src",
  "tags": ["type:app", "scope:tuffz-info"],
  "implicitDependencies": ["ericbuettner-com"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/tuffz-info-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "ericbuettner-com:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "ericbuettner-com:serve:production"
        },
        "ci": {
          "devServerTarget": "ericbuettner-com:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tuffz-info-e2e/**/*.{js,ts}"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore({projectName}): ðŸš€ release version ${version} [skip ci]",
        "preset": "conventional",
        "skipCommitTypes": ["build", "chore", "ci", "docs", "style"]
      }
    }
  }
}
