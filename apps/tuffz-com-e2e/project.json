{
  "name": "tuffz-com-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tuffz-com-e2e/src",
  "projectType": "application",
  "tags": ["type:e2e", "scope:tuffz-com"],
  "implicitDependencies": ["tuffz-com"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tuffz-com-e2e/**/*.{js,ts}"]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/tuffz-com-e2e/cypress.config.ts",
        "devServerTarget": "tuffz-com:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "tuffz-com:serve:production"
        },
        "ci": {
          "devServerTarget": "tuffz-com:serve-static"
        }
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore({projectName}): ðŸš€ release version ${version} [skip ci]",
        "preset": "conventional",
        "skipCommitTypes": ["build", "chore", "ci", "docs", "style"]
      }
    }
  }
}
