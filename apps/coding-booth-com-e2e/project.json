{
  "name": "coding-booth-com-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/coding-booth-com-e2e/src",
  "tags": ["type:app", "scope:coding-booth-com"],
  "implicitDependencies": ["coding-booth-com"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/coding-booth-com-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "coding-booth-com:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "coding-booth-com:serve:production"
        },
        "ci": {
          "devServerTarget": "coding-booth-com:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "main",
        "preset": {
          "types": [
            { "type": "feat", "section": "‚ú® Features" },
            { "type": "fix", "section": "üêû Bug Fixes" },
            { "type": "chore", "section": "‚öôÔ∏è Chores" },
            { "type": "build", "section": "üõ†Ô∏è Build System" },
            { "type": "ci", "section": "ü§ñ Continuous Integration" },
            { "type": "docs", "section": "üìÑ Documentation" },
            { "type": "style", "section": "üé® Styles" },
            { "type": "refactor", "section": "üßπ Code Refactoring" },
            { "type": "perf", "section": "‚ö°Ô∏è Performance Improvements" },
            { "type": "test", "section": "‚úÖ Tests" }
          ]
        },
        "commitMessageFormat": "chore({projectName}): üöÄ release version [skip ci]",
        "skipCommitTypes": ["build", "chore", "ci", "docs", "style"]
      }
    }
  }
}
