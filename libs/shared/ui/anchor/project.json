{
  "name": "ui-anchor",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ui/anchor/src",
  "projectType": "library",
  "tags": ["type:ui", "scope:shared"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/ui/anchor/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/ui/anchor/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore({projectName}): ðŸš€ release version ${version} [skip ci]",
        "preset": "conventional",
        "skipCommitTypes": ["build", "chore", "ci", "docs", "style"]
      }
    }
  }
}
