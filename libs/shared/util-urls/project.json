{
  "name": "shared-util-urls",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util-urls/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:util"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/util-urls",
        "main": "libs/shared/util-urls/src/index.ts",
        "tsConfig": "libs/shared/util-urls/tsconfig.lib.json",
        "assets": ["libs/shared/util-urls/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/util-urls/jest.config.ts"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "main",
        "commitMessageFormat": "chore({projectName}): üöÄ release version ${version} [skip ci]",
        "preset": {
          "types": [
            {
              "type": "feat",
              "section": "‚ú® Features"
            },
            {
              "type": "fix",
              "section": "üêõ Bug Fixes"
            },
            {
              "type": "docs",
              "section": "üìù Documentation"
            },
            {
              "type": "refactor",
              "section": "‚ôªÔ∏è Code Refactoring"
            },
            {
              "type": "perf",
              "section": "‚ö°Ô∏è Performance Improvements"
            },
            {
              "type": "test",
              "section": "‚úÖ Tests"
            }
          ]
        },
        "skipCommitTypes": ["build", "chore", "ci", "docs", "style", "test"]
      }
    }
  }
}
